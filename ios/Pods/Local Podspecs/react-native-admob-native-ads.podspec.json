    "ios": "9.0",
    "tvos": "9.2"
  },
  "source": {
    "git": "https://github.com/ammarahm-ed/react-native-admob-native-ads.git",
    "tag": "v0.6.0"
  },
  "source_files": "ios/**/*.{h,m}",
  "weak_frameworks": [
